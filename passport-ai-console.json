  "integrations": [
    { "type": "docker", "service": "open-webui", "port": 3000 },
    { "type": "service", "service": "ollama", "port": 11434 }
  ],
  
  "settings": {
    "enabled": true,
    "layout": "hybrid",
    "storageKey": "ai-console-settings",
    "sections": [
      { "id": "general", "label": "General", "icon": "‚öôÔ∏è" },
      { "id": "models", "label": "Models", "icon": "üß†" },
      { "id": "providers", "label": "Providers", "icon": "üîå" }
    ]
  },
  
  "health": {
    "enabled": true,
    "endpoint": "/health",
    "interval": 30000,
    "checks": [
      { "name": "ollama", "type": "http", "target": "http://localhost:11434/api/tags", "critical": false },
      { "name": "open-webui", "type": "http", "target": "http://localhost:3000/health", "critical": false }
    ]
  },
  
  "discovery": {
    "enabled": true,
    "scanPaths": ["./cartridges/"],
    "autoLoad": true,
    "priority": 50
